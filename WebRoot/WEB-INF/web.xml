<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <servlet-name>AddContactServlet</servlet-name>
    <servlet-class>com.shenji.contact.servlet.AddContactServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ListContactServlet</servlet-name>
    <servlet-class>com.shenji.contact.servlet.ListContactServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>AddContactServlet</servlet-name>
    <url-pattern>/AddContactServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ListContactServlet</servlet-name>
    <url-pattern>/ListContactServlet</url-pattern>
  </servlet-mapping>

</web-app>